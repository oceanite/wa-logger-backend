{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ],
    "functions": {
      "api/*.js": {
        "runtime": "nodejs18.x",
        "memory": 512,
        "maxDuration": 10
      }
    }
  }
